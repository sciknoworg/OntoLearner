<?xml version="1.0"?>
<rdf:RDF xmlns="https://w3id.org/icon/ontology/"
     xml:base="https://w3id.org/icon/ontology/"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:crm="http://erlangen-crm.org/current/"
     xmlns:dct="https://www.w3.org/1999/xhtml/vocab#dct:"
     xmlns:dul="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:icon="https://w3id.org/icon/ontology/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:vocab="https://www.w3.org/1999/xhtml/vocab#"
     xmlns:schema="http://schema.org/"
     xmlns:dcterms="http://purl.org/dc/terms/"
     xmlns:cpannotationschema="http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#">
    <owl:Ontology rdf:about="https://w3id.org/icon/ontology/">
        <owl:versionIRI rdf:resource="https://w3id.org/icon/ontology/1.1.0/"/>
        <owl:imports rdf:resource="http://erlangen-crm.org/current/"/>
        <owl:imports rdf:resource="http://purl.org/emmedi/hico/2020-03-31"/>
        <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl"/>
        <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
        <owl:imports rdf:resource="https://www.w3id.org/simulation/ontology/"/>
        <dc:contributor xml:lang="en">Aldo Gangemi</dc:contributor>
        <dc:contributor xml:lang="en">Marieke van Erp</dc:contributor>
        <dc:creator>Bruno Sartini</dc:creator>
        <dc:creator>Sofia Baroncini</dc:creator>
        <dcterms:created xml:lang="en">April 12th, 2022</dcterms:created>
        <dcterms:issued xml:lang="en">April 12th, 2022</dcterms:issued>
        <dcterms:license xml:lang="en">https://creativecommons.org/licenses/by/4.0/</dcterms:license>
        <dcterms:publisher xml:lang="en">Bruno Sartini</dcterms:publisher>
        <dcterms:title xml:lang="en">ICON Ontology</dcterms:title>
        <vann:preferredNamespacePrefix xml:lang="en">icon</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri xml:lang="en">https://w3id.org/icon/ontology/</vann:preferredNamespaceUri>
        <schema:citation xml:lang="en">Sartini B. and Baroncini S. Icon Ontology. https://w3id.org/icon/ontology</schema:citation>
        <rdfs:comment xml:lang="en">The ICON ontology deals with high granularity art interpretation. It was developed by conceptualizing Panofsky&apos;s theory of levels of interpretation, therefore artworks can be described according to Pre-iconographical, Iconographical and Iconological information.</rdfs:comment>
        <owl:backwardCompatibleWith xml:lang="en">First version</owl:backwardCompatibleWith>
        <owl:priorVersion xml:lang="en">1.0</owl:priorVersion>
        <owl:versionInfo xml:lang="en">1.1.0</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/created -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/created"/>
    


    <!-- http://purl.org/dc/terms/issued -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/issued"/>
    


    <!-- http://purl.org/dc/terms/license -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/license"/>
    


    <!-- http://purl.org/dc/terms/title -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/title"/>
    


    <!-- http://www.w3.org/2001/XMLSchema#citation -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2001/XMLSchema#citation"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/spar/cito/citesAsEvidence -->

    <owl:ObjectProperty rdf:about="http://purl.org/spar/cito/citesAsEvidence">
        <rdfs:domain rdf:resource="https://w3id.org/icon/ontology/Recognition"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/spar/cito/citesForInformation -->

    <owl:ObjectProperty rdf:about="http://purl.org/spar/cito/citesForInformation">
        <rdfs:domain rdf:resource="https://w3id.org/icon/ontology/Recognition"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/spar/cito/givesSupportTo -->

    <owl:ObjectProperty rdf:about="http://purl.org/spar/cito/givesSupportTo">
        <rdfs:domain rdf:resource="https://w3id.org/icon/ontology/Recognition"/>
        <rdfs:range rdf:resource="https://w3id.org/icon/ontology/Recognition"/>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/vir#K4_is_visual_prototype_of -->

    <owl:ObjectProperty rdf:about="http://w3id.org/vir#K4_is_visual_prototype_of"/>
    


    <!-- https://w3id.org/icon/ontology/aboutWorkOfArt -->

    <owl:ObjectProperty rdf:about="https://w3id.org/icon/ontology/aboutWorkOfArt">
        <owl:inverseOf rdf:resource="https://w3id.org/icon/ontology/interpretedByRecognition"/>
        <rdfs:domain rdf:resource="https://w3id.org/icon/ontology/Recognition"/>
        <rdfs:range rdf:resource="https://w3id.org/icon/ontology/Artwork"/>
        <rdfs:comment xml:lang="en">This relation associates each Recognition directly to the artwork. In this way, it is possible to retrieve easily all the interpretations about an artwork. Therefore, it a highly recommended good practice to declare it for each interpretation.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/icon/ontology/ICONOntology"/>
        <rdfs:label xml:lang="en">About Work Of Art</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/icon/ontology/associatedForm -->

    <owl:ObjectProperty rdf:about="https://w3id.org/icon/ontology/associatedForm">
        <rdfs:subPropertyOf rdf:resource="http://erlangen-crm.org/current/P140_assigned_attribute_to"/>
        <owl:inverseOf rdf:resource="https://w3id.org/icon/ontology/isPureFormOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/icon/ontology/PreiconographicalRecognition"/>
        <rdfs:range rdf:resource="https://w3id.org/icon/ontology/PureForm"/>
        <rdfs:comment>This relation associates a Pure Form to the Pre-iconographical Recognition that describes it at the first level of interpretation.</rdfs:comment>
        <rdfs:label xml:lang="en">Associated Form</rdfs:label>
        <owl:deprecated>Since 1.1 version</owl:deprecated>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/icon/ontology/composedOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/icon/ontology/composedOf">
        <owl:inverseOf rdf:resource="https://w3id.org/icon/ontology/takesPartIn"/>
        <rdfs:domain rdf:resource="https://w3id.org/icon/ontology/Invenzione"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/icon/ontology/Character"/>
                    <rdf:Description rdf:about="https://w3id.org/icon/ontology/Event"/>
                    <rdf:Description rdf:about="https://w3id.org/icon/ontology/NamedObject"/>
                    <rdf:Description rdf:about="https://w3id.org/icon/ontology/Personification"/>
                    <rdf:Description rdf:about="https://w3id.org/icon/ontology/Place"/>
                    <rdf:Description rdf:about="https://w3id.org/icon/ontology/Symbol"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">This Property links an invenzione with its components, which may vary from Characters, Events, Named Objects, Places to Personifications, Symbols according to the kind of Invenzione is being described.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/icon/ontology/ICONOntology"/>
        <rdfs:label xml:lang="en">Composed Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/icon/ontology/hasArtisticMotif -->

    <owl:ObjectProperty rdf:about="https://w3id.org/icon/ontology/hasArtisticMotif">
        <owl:inverseOf rdf:resource="https://w3id.org/icon/ontology/isArtisticMotifOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/icon/ontology/IntrinsicMeaning"/>
        <rdfs:range rdf:resource="https://w3id.org/icon/ontology/ArtisticMotif"/>
        <rdfs:comment xml:lang="en">This relation links an Intrinsic Meaning to the Artistic Motif recognized at the first level of interpretation. It should be used to express that the third level meaning associated to the Intrinsic Meaning refers specifically to this Artistic Motif, and not only to the general artwork.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/icon/ontology/ICONOntology"/>
        <rdfs:label xml:lang="en">Has Artistic Motif</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/icon/ontology/hasCharacter -->

    <owl:ObjectProperty rdf:about="https://w3id.org/icon/ontology/hasCharacter">
        <owl:inverseOf rdf:resource="https://w3id.org/icon/ontology/isCharacterOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/icon/ontology/Image"/>
        <rdfs:range rdf:resource="https://w3id.org/icon/ontology/Character"/>
        <skos:broadMatch rdf:resource="http://erlangen-crm.org/current/P138_represents"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/icon/ontology/hasComposition -->

    <owl:ObjectProperty rdf:about="https://w3id.org/icon/ontology/hasComposition">
        <owl:inverseOf rdf:resource="https://w3id.org/icon/ontology/isCompositionOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/icon/ontology/IntrinsicMeaning"/>
        <rdfs:range rdf:resource="https://w3id.org/icon/ontology/Composition"/>
        <rdfs:comment xml:lang="en">This relation links an Intrinsic Meaning to the Composition recognized at the first level of interpretation. It should be used to express that the third level meaning associated to the Intrinsic Meaning refers specifically to this Composition, and not only to the general artwork.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/icon/ontology/ICONOntology"/>
        <rdfs:label xml:lang="en">Has Composition</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/icon/ontology/hasCompositionalStructure -->

    <owl:ObjectProperty rdf:about="https://w3id.org/icon/ontology/hasCompositionalStructure">
        <owl:inverseOf rdf:resource="https://w3id.org/icon/ontology/isCompositionalStructureOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/icon/ontology/Composition"/>
        <rdfs:range rdf:resource="https://w3id.org/icon/ontology/CompositionalStructure"/>
        <rdfs:comment>This relation describes the arrangement of the Artistic Motifs composing the Composition. It is suggested to use it only when they are arranged in a relevant observable structure (e.g. pyramidal, oblique)</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/icon/ontology/ICONOntology"/>
        <rdfs:label xml:lang="en">Has Compositional Structure</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/icon/ontology/hasCopiedMotif -->

    <owl:ObjectProperty rdf:about="https://w3id.org/icon/ontology/hasCopiedMotif">
        <owl:inverseOf rdf:resource="https://w3id.org/icon/ontology/isCopiedMotifOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/icon/ontology/FormalMotifRecognition"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/icon/ontology/ArtisticMotif"/>
                    <rdf:Description rdf:about="https://w3id.org/icon/ontology/Composition"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <skos:broadMatch rdf:resource="http://w3id.org/vir#K4_is_visual_prototype_of"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/icon/ontology/hasEvent -->

    <owl:ObjectProperty rdf:about="https://w3id.org/icon/ontology/hasEvent">
        <owl:inverseOf rdf:resource="https://w3id.org/icon/ontology/isEventOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/icon/ontology/Image"/>
        <rdfs:range rdf:resource="https://w3id.org/icon/ontology/Event"/>
        <skos:broadMatch rdf:resource="http://erlangen-crm.org/current/P138_represents"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/icon/ontology/hasExpressionalMeaning -->

    <owl:ObjectProperty rdf:about="https://w3id.org/icon/ontology/hasExpressionalMeaning">
        <owl:inverseOf rdf:resource="https://w3id.org/icon/ontology/isExpressionalMeaningOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/icon/ontology/ArtisticMotif"/>
                    <rdf:Description rdf:about="https://w3id.org/icon/ontology/Composition"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/icon/ontology/ExpressionalQuality"/>
    <skos:broadMatch rdf:resource="http://erlangen-crm.org/current/P138_represents"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/icon/ontology/hasFactualMeaning -->

    <owl:ObjectProperty rdf:about="https://w3id.org/icon/ontology/hasFactualMeaning">
        <owl:inverseOf rdf:resource="https://w3id.org/icon/ontology/isFactualMeaningOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/icon/ontology/ArtisticMotif"/>
                    <rdf:Description rdf:about="https://w3id.org/icon/ontology/Composition"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/icon/ontology/Action"/>
                    <rdf:Description rdf:about="https://w3id.org/icon/ontology/NaturalElement"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <skos:broadMatch rdf:resource="http://erlangen-crm.org/current/P138_represents"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/icon/ontology/hasImage -->

    <owl:ObjectProperty rdf:about="https://w3id.org/icon/ontology/hasImage">
        <owl:inverseOf rdf:resource="https://w3id.org/icon/ontology/isImageOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/icon/ontology/IntrinsicMeaning"/>
        <rdfs:range rdf:resource="https://w3id.org/icon/ontology/Image"/>
        <rdfs:comment xml:lang="en">This relation links an Intrinsic Meaning to the Image recognized at the second level of interpretation. It should be used to express that the third level meaning associated to the Intrinsic Meaning refers specifically to this Image, and not only to the general artwork.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/icon/ontology/ICONOntology"/>
        <rdfs:label xml:lang="en">Has Image</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/icon/ontology/hasInvenzione -->

    <owl:ObjectProperty rdf:about="https://w3id.org/icon/ontology/hasInvenzione">
        <owl:inverseOf rdf:resource="https://w3id.org/icon/ontology/isInvenzioneOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/icon/ontology/IntrinsicMeaning"/>
        <rdfs:range rdf:resource="https://w3id.org/icon/ontology/Invenzione"/>
        <rdfs:comment xml:lang="en">This relation links an Intrinsic Meaning to the Invenzione recognized at the second level of interpretation. It should be used to express that the third level meaning associated to the Intrinsic Meaning refers specifically to this invenzione, and not only to the general artwork.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/icon/ontology/ICONOntology"/>
        <rdfs:label xml:lang="en">Has Invenzione</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/icon/ontology/hasNamedObject -->

    <owl:ObjectProperty rdf:about="https://w3id.org/icon/ontology/hasNamedObject">
        <owl:inverseOf rdf:resource="https://w3id.org/icon/ontology/isNamedObjectOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/icon/ontology/Image"/>
        <rdfs:range rdf:resource="https://w3id.org/icon/ontology/NamedObject"/>
        <skos:broadMatch rdf:resource="http://erlangen-crm.org/current/P138_represents"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/icon/ontology/hasPart -->

    <owl:ObjectProperty rdf:about="https://w3id.org/icon/ontology/hasPart">
        <rdfs:subPropertyOf rdf:resource="http://erlangen-crm.org/current/P106_is_composed_of"/>
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasPart"/>
        <owl:inverseOf rdf:resource="https://w3id.org/icon/ontology/partOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/icon/ontology/Composition"/>
        <rdfs:range rdf:resource="https://w3id.org/icon/ontology/ArtisticMotif"/>
        <rdfs:comment xml:lang="en">This relation links a Composition to the Artistic Motifs of which it consists.</rdfs:comment>
        <rdfs:label xml:lang="en">Has Part (ICON)</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/icon/ontology/hasPersonification -->

    <owl:ObjectProperty rdf:about="https://w3id.org/icon/ontology/hasPersonification">
        <owl:inverseOf rdf:resource="https://w3id.org/icon/ontology/isPersonificationOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/icon/ontology/Image"/>
        <rdfs:range rdf:resource="https://w3id.org/icon/ontology/Personification"/>
        <skos:broadMatch rdf:resource="http://erlangen-crm.org/current/P138_represents"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/icon/ontology/hasPlace -->

    <owl:ObjectProperty rdf:about="https://w3id.org/icon/ontology/hasPlace">
        <owl:inverseOf rdf:resource="https://w3id.org/icon/ontology/isPlaceOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/icon/ontology/Image"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://erlangen-crm.org/current/E53_Place"/>
                    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Place"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <skos:broadMatch rdf:resource="http://erlangen-crm.org/current/P138_represents"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/icon/ontology/hasPrototypicalMotif -->

    <owl:ObjectProperty rdf:about="https://w3id.org/icon/ontology/hasPrototypicalMotif">
        <owl:inverseOf rdf:resource="https://w3id.org/icon/ontology/isPrototypicalMotifOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/icon/ontology/FormalMotifRecognition"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/icon/ontology/ArtisticMotif"/>
                    <rdf:Description rdf:about="https://w3id.org/icon/ontology/Composition"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <skos:broadMatch rdf:resource="http://w3id.org/vir#K4_is_visual_prototype_of"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/icon/ontology/hasRecAttribute -->

    <owl:ObjectProperty rdf:about="https://w3id.org/icon/ontology/hasRecAttribute">
        <owl:inverseOf rdf:resource="https://w3id.org/icon/ontology/isRecAttributeOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/icon/ontology/Image"/>
        <rdfs:range rdf:resource="https://w3id.org/icon/ontology/ArtisticMotif"/>
        <rdfs:comment xml:lang="en">This relation identifies objects belonging to the level 1 as fundamental for the recognition of a certain subject. It is suggested that the Artistic Motif of the object is included in the Composition corresponding to the subject.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/icon/ontology/ICONOntology"/>
        <rdfs:label xml:lang="en">Has Rec Attribute</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/icon/ontology/hasSymbol -->

    <owl:ObjectProperty rdf:about="https://w3id.org/icon/ontology/hasSymbol">
        <owl:inverseOf rdf:resource="https://w3id.org/icon/ontology/isSymbolOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/icon/ontology/Image"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/icon/ontology/Symbol"/>
                    <rdf:Description rdf:about="https://www.w3id.org/simulation/ontology/Simulation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <skos:broadMatch rdf:resource="http://erlangen-crm.org/current/P138_represents"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/icon/ontology/iconographicallyCompliesWith -->

    <owl:ObjectProperty rdf:about="https://w3id.org/icon/ontology/iconographicallyCompliesWith">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#satisfies"/>
        <owl:inverseOf rdf:resource="https://w3id.org/icon/ontology/isCompliantWithIconographicalRecognition"/>
        <rdfs:domain rdf:resource="https://w3id.org/icon/ontology/IconographicalRecognition"/>
        <rdfs:range rdf:resource="https://w3id.org/icon/ontology/InterpretationDescription"/>
        <rdfs:comment xml:lang="en">This property is used to link all the iconographical recognitions that take part in one or more interpretation(s), to that/those interpretation(s)</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/icon/ontology/ICONOntology"/>
        <rdfs:label xml:lang="en">iconographically complies with</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/icon/ontology/iconologicallyCompliesWith -->

    <owl:ObjectProperty rdf:about="https://w3id.org/icon/ontology/iconologicallyCompliesWith">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#satisfies"/>
        <owl:inverseOf rdf:resource="https://w3id.org/icon/ontology/isCompliantWithIconologicalRecognition"/>
        <rdfs:domain rdf:resource="https://w3id.org/icon/ontology/IconologicalRecognition"/>
        <rdfs:range rdf:resource="https://w3id.org/icon/ontology/InterpretationDescription"/>
        <rdfs:comment xml:lang="en">This property is used to link all the iconological recognitions that take part in one or more interpretation(s), to that/those interpretation(s)</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/icon/ontology/ICONOntology"/>
        <rdfs:label xml:lang="en">iconologically complies with</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/icon/ontology/interpretedByRecognition -->

    <owl:ObjectProperty rdf:about="https://w3id.org/icon/ontology/interpretedByRecognition">
        <rdfs:domain rdf:resource="https://w3id.org/icon/ontology/Artwork"/>
        <rdfs:range rdf:resource="https://w3id.org/icon/ontology/Recognition"/>
        <rdfs:comment xml:lang="en">Inverse property of aboutWorkOfArt</rdfs:comment>
