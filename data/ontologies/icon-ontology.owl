                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/icon/ontology/Artwork"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:broadMatch rdf:resource="http://erlangen-crm.org/current/E13_Attribute_Assignment"/>
    </owl:Class>
    


