                        <owl:someValuesFrom rdf:resource="https://w3id.org/icon/ontology/Character"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://w3id.org/icon/ontology/composedOf"/>
                        <owl:someValuesFrom rdf:resource="https://w3id.org/icon/ontology/Event"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://w3id.org/icon/ontology/composedOf"/>
                        <owl:someValuesFrom rdf:resource="https://w3id.org/icon/ontology/NamedObject"/>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <skos:broadMatch rdf:resource="http://erlangen-crm.org/current/E1_CRM_Entity"/>
    </owl:Class>
    


    <!-- https://w3id.org/icon/ontology/Symbol -->

    <owl:Class rdf:about="https://w3id.org/icon/ontology/Symbol">
        <owl:equivalentClass rdf:resource="https://www.w3id.org/simulation/ontology/Simulation"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/arco/ontology/context-description/Subject"/>
